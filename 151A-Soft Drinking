#include <bits/stdc++.h>
using namespace std;

int main() {
    int n, k, l, c, d, p, nl, np;
    cin >> n >> k >> l >> c >> d >> p >> nl >> np;

    int total_drink_toasts = (k * l) / nl;
    int total_lime_toasts = c * d;
    int total_salt_toasts = p / np;

    int toasts_per_friend = min({total_drink_toasts / n, total_lime_toasts / n, total_salt_toasts / n});

    cout << toasts_per_friend << "\n";
    return 0;
}
