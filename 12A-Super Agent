#include <bits/stdc++.h>
using namespace std;

int main() {
    char g[3][3];
    for(int i=0;i<3;i++)
        for(int j=0;j<3;j++)
            cin >> g[i][j];

    int ok = 1;
    for(int i=0;i<3;i++)
        for(int j=0;j<3;j++) {
            int ii = 2 - i;
            int jj = 2 - j;
            if (g[i][j] != g[ii][jj])
                ok = 0;
        }

    cout << (ok ? "YES" : "NO");
    return 0;
}
