#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin >> t;

    string target = "codeforces";
    set<char> letters(target.begin(), target.end());

    while(t--) {
        char c;
        cin >> c;
        if(letters.count(c))
            cout << "YES\n";
        else
            cout << "NO\n";
    }

    return 0;
}
