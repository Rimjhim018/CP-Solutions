#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin >> n;
    vector<int> a(n);
    for(int i = 0; i < n; i++) cin >> a[i];

    int q;
    cin >> q;
    while(q--) {
        int l, r, x, y;
        cin >> l >> r >> x >> y;
        l--; r--; // convert to 0-based indexing
        for(int i = l; i <= r; i++) {
            if(a[i] == x) a[i] = y;
        }
    }

    for(int i = 0; i < n; i++) {
        cout << a[i] << " ";
    }
    cout << endl;
}
