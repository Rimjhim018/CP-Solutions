#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin >> t;
    while(t--) {
        string s;
        cin >> s;
        if(s == "YES" || s == "Yes" || s == "yES" || s == "yeS" || s == "YEs" || s == "YeS" || s == "yEs" || s == "yes")
            cout << "YES\n";
        else
            cout << "NO\n";
    }
}
